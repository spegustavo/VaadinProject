#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules/semver/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/core-js-compat@3.17.3/node_modules/core-js-compat/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/core-js-compat@3.17.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules/semver/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/semver@7.0.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/core-js-compat@3.17.3/node_modules/core-js-compat/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/core-js-compat@3.17.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
fi
