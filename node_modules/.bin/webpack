#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules/webpack/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules/webpack/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/webpack/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/webpack/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules/webpack/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules/webpack/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.12/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/webpack/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/webpack/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
