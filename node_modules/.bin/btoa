#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/btoa/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/btoa/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/btoa@1.2.1/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/btoa/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/btoa/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../btoa/bin/btoa.js" "$@"
else
  exec node  "$basedir/../btoa/bin/btoa.js" "$@"
fi
