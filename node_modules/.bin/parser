#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/parser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/parser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/parser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/parser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/parser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/parser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules/@babel/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/parser@7.15.6/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/@babel/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/parser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/parser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/@babel/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
