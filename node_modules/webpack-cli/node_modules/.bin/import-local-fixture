#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.46.0/node_modules/webpack-cli/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.46.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/import-local@2.0.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.46.0/node_modules/webpack-cli/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.46.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
