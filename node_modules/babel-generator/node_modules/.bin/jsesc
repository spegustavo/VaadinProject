#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-generator@6.26.1/node_modules/babel-generator/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-generator@6.26.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/jsesc@1.3.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-generator@6.26.1/node_modules/babel-generator/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-generator@6.26.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
fi
