#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/dist/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/dist/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/workbox-build@6.3.0/node_modules/workbox-build/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/workbox-build@6.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/dist/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/dist/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules/rollup/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup@2.56.3/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/workbox-build@6.3.0/node_modules/workbox-build/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/workbox-build@6.3.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.56.3/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.56.3/node_modules/rollup/dist/bin/rollup" "$@"
fi
