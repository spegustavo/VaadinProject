#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules/terser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules/terser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup-plugin-terser@7.0.2_rollup@2.56.3/node_modules/rollup-plugin-terser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup-plugin-terser@7.0.2_rollup@2.56.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules/terser/bin/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules/terser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/terser@5.8.0/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup-plugin-terser@7.0.2_rollup@2.56.3/node_modules/rollup-plugin-terser/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/rollup-plugin-terser@7.0.2_rollup@2.56.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.8.0/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@5.8.0/node_modules/terser/bin/terser" "$@"
fi
