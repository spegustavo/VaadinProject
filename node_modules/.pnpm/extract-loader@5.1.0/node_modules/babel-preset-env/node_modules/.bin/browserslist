#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/browserslist@3.2.8/node_modules/browserslist/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/browserslist@3.2.8/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-preset-env@1.7.0/node_modules/babel-preset-env/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-preset-env@1.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/browserslist@3.2.8/node_modules/browserslist/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/browserslist@3.2.8/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules:/mnt/c/Desenvolvimento/work_vaadin/node_modules:/mnt/c/Desenvolvimento/node_modules:/mnt/c/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-preset-env@1.7.0/node_modules/babel-preset-env/node_modules:/mnt/c/Desenvolvimento/work_vaadin/webapp/node_modules/.pnpm/babel-preset-env@1.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@3.2.8/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@3.2.8/node_modules/browserslist/cli.js" "$@"
fi
